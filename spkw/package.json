  "scripts": {
    "watch-sass": "sass --watch src/sass/:dist/css/",
    "watch-jade": "jade --watch src/jade/*.jade -o dist/html/",
    "build-js": "browserify src/js/Default.js -o dist/js/Default.js & browserify src/js/Template.js -o dist/js/Template.js",
