  },
  "dependencies": {
    "browserify": "^13.0.0",
    "d3": "^3.5.16",
    "jade": "^1.11.0",
    "jquery": "^2.2.0",
    "nodemon": "^1.8.1",
    "normalize-scss": "^4.0.3",
    "nouislider": "^8.5.1",
    "shortid": "^2.2.4",
    "three": "^0.93.0",
    "three-orbit-controls": "^72.0.0",
    "tween.js": "^16.3.4",
    "wolfy87-eventemitter": "^4.3.0"
  }
}
